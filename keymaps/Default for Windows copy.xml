<keymap version="1" name="Default for Windows copy" parent="$default">
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt t" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl caps_lock" />
  </action>
  <action id="NewElement">
    <keyboard-shortcut first-keystroke="alt n" />
  </action>
  <action id="OpenFile">
    <keyboard-shortcut first-keystroke="shift o" />
  </action>
  <action id="TogglePowerSave">
    <keyboard-shortcut first-keystroke="alt p" />
  </action>
</keymap>